{
  "preset": "psr12",
  "rules": {
    "concat_space": {
      "spacing": "one"
    },
    "declare_strict_types": true,
    "final_class": false,
    "no_unused_imports": true,
    "ordered_imports": {
      "sort_algorithm": "alpha"
    },
    "phpdoc_align": {
      "align": "left"
    },
    "trailing_comma_in_multiline": {
      "elements": ["arguments", "arrays", "match", "parameters"]
    },
    "void_return": true
  }
}
